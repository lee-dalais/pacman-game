{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "lib": [  "es2015", "es2016" ],
    "allowJs": true,
    "checkJs": true,
    "rootDir": "./",
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true
  },
  "include": [
    "*.ts",
    "./models/*.ts"
  ],
  "exclude": [ "./node_modules/*" ]
  
}